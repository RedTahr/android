<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:top="0dp"
        android:left="0dp"
        android:right="0dp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:top="19sp"
        android:left="10dp"
        android:right="10dp"
        android:bottom="19sp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%" >
            <shape android:shape="rectangle">
                <solid android:color="@android:color/white" />
            </shape>
        </rotate>
    </item>
    <item android:top="10dp"
        android:left="19sp"
        android:right="19sp"
        android:bottom="10dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%" >
            <shape android:shape="rectangle">
                <solid android:color="@android:color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>